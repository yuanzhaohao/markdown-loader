{
  "name": "markdown-website-loader",
  "version": "0.0.6",
  "description": "A simple markdown loader.",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "node example/dev.js",
    "format": "prettier './{example,lib}/**/*.js' --write"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuanzhaohao/markdown-loader.git"
  },
  "keywords": [
    "markdown",
    "loader",
    "webpack",
    "react"
  ],
  "author": "yuanzhaohao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuanzhaohao/markdown-loader/issues"
  },
  "homepage": "https://github.com/yuanzhaohao/markdown-loader#readme",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/react": "^16.7.1",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^1.0.0",
    "dashkit-ui": "0.0.12",
    "express": "^4.16.3",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.10.0",
    "prettier": "^1.17.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-svg-loader": "^2.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^3.5.0",
    "typescript": "^3.1.6",
    "webpack": "^3.12.0",
    "webpack-dev-middleware": "^2.0.6"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/generator": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/traverse": "^7.4.5",
    "@babel/types": "^7.4.4",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.3",
    "js-yaml": "^3.13.1",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.4.2"
  }
}
